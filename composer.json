{
  "name": "kicken/gearman-php",
  "type": "library",
  "description": "A PHP implementation of the Gearman protocol.",
  "license": "MIT",
  "authors": [
    {
      "name": "Keith Maika"
    }
  ],
  "autoload": {
    "psr-4": {
      "Kicken\\Gearman\\": "src/"
    },
    "files": [
      "src/utility.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Kicken\\Gearman\\Test\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.4.0",
    "react/event-loop": "^1.0",
    "react/promise": "^2.8.0",
    "psr/log": "^1.0 || ^2.0",
    "symfony/event-dispatcher": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "ext-json": "*"
  },
  "extra": {
    "branch-alias": {
      "dev-v2-rewrite": "2.0.x-dev"
    }
  }
}
